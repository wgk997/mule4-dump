#%RAML 1.0 Library
types:
  
  procedureCodeResponseForGet:
    type: object
    properties:
      statusCode:
        type: number
        minimum: 100
        maximum: 599
        required: true
        example:
          200
      message:
        type: string
        required: true      
        example:
          OK     
      procedureCodes:
        required: true
        type: array
        items: !include procedureCodeDataType.raml 

