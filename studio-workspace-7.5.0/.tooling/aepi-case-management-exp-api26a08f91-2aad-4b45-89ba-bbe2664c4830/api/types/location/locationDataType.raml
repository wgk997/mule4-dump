#%RAML 1.0 DataType
type: object
properties: 
  lat:
    type: number
    required: true
    example:
      37.4267861
  lng:
    type: number
    required: true
    example:
      -122.0806032
  locationType:
    type: string
    required: false
    example:
      ROOFTOP
    enum:
      - ROOFTOP
      - APPROXIMATE
