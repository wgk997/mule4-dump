#%RAML 1.0 DataType
type: object
properties: 
  smsMessage:
    type: string
    required: true
    minLength: 5
    maxLength: 1600
    example:
      "Your test are complete"
  cellNumbers:
    type: array
    minItems: 1
    required: true
    example:
      [ "+19725550000", "+19725550001", "+19725550002" ]
