#%RAML 1.0 DataType
type: object
properties: 
  to:
    type: string
    example:
      "+9725550000"
  status:
    type: string
    enum:
      - ACCEPTED
      - DELIVERED
      - FAILED
      - PARTIALLY_DELIVERED
      - QUEUED
      - READ
      - READING
      - RECEIVING
      - SCHEDULED
      - SENT
      - UNDELIVERED
    example:
      SENT
