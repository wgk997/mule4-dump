#%RAML 1.0 DataType
description: A product review
type: object
properties:
  id:
    type: integer
    required: false
    example: 801
  productId:
    type: integer
    required: false
    example: 1001
  rating:
    type: integer
    minimum: 0
    maximum: 5
    example: 2
  review:
    type: string
    example: "Very good quality products. Happy with Purchase."
    minLength: 5
  createdDate:
    type: string
    required: false
    example: "2019-06-13"