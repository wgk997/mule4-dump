    <element name="CreateUnicenterIncidentProcessRequest">
        <complexType>
            <all>
                <!-- this is not used anymore but it's in here to avoid breaking legacy consumers -->
                <element name="input"
                         minOccurs="0"
                         type="string"/>
                <!-- this is not used anymore but it's in here to avoid breaking legacy consumers -->
                <element name="req"
                         minOccurs="0"
                         type="string"/>
                <!-- this is not used anymore but it's in here to avoid breaking legacy consumers -->
                <element name="value"
                         minOccurs="0"
                         type="string"/>
                <!-- basic Request Detail header information -->
                <element name="requester"
                         type="string"
                         minOccurs="0"/>
                <element name="affectedEndUser"
                         type="string"
                         minOccurs="0"/>
                <element name="incidentArea"
                         type="string"/>
                <element name="summary"
                         type="string"/>
                <element name="description"
                         type="string"/>
                <!-- Only used for error emails. Create Fuel/others do not supply this -->
                <element name="bpelName"
                         minOccurs="0"
                         type="string"/>
                <element name="instanceID"
                         type="string"
                         minOccurs="0"/>
                <!-- Incident Request Detail for Apps.EAM.Fuel pcat:2243749 TRAK Fuel property information.
                 Only supplied by the CreateFuel application/service
                 -->
                <element name="FuelSystem_Fuel"
                         minOccurs="0">
                    <complexType>
                        <all>
                            <!-- CreateFuel, when using pcat 2243751 to report meter reading/miles/hour incidents,
                             only supplies ADDFUEL_EQUIPMENTCODE, so making everything else in here optional -->

                            <!--  ADDFUEL_DEPOTCODE, maxLength allowed is 30  -->
                            <element name="ADDFUEL_DEPOTCODE"
                                     minOccurs="0"
                                     type="string"/>
                            <!--  ADDFUEL_PUMPID, maxLength allowed is 30 -->
                            <element name="ADDFUEL_PUMPCODE"
                                     minOccurs="0"
                                     type="string"/>
                            <!--  ADDFUEL_FUELID, maxLength allowed is 30 -->
                            <element name="ADDFUEL_FUELCODE"
                                     minOccurs="0"
                                     type="string"/>
                            <!--  ADDFUEL_VEHICLEID, maxLength allowed is 30 -->
                            <element name="ADDFUEL_EQUIPMENTCODE"
                                     type="string"/>
                            <!--  ADDFUEL_ISSUETO, maxLength allowed is 15 -->
                            <element name="ADDFUEL_PERSONCODE"
                                     minOccurs="0"
                                     type="string"/>
                            <!--  ADDFUEL_QUANTITYISSUED -->
                            <element name="ADDFUEL_QUANTITY_VALUE"
                                     minOccurs="0"
                                     type="decimal"/>
                            <!-- ADDFUEL_TRANSACTIONDATE-->
                            <element name="ADDFUEL_TRANSACTION_DATE"
                                     minOccurs="0"
                                     type="dateTime"/>
                            <!--  ADDFUEL_PRICE -->
                            <element name="ADDFUEL_PRICE_VALUE"
                                     minOccurs="0"
                                     type="decimal"/>
                        </all>
                    </complexType>
                </element>
                <!-- Schema definition from INFOR -->
                <!-- Incident Request Detail for Apps.EAM.Meter pcat:2243751 TRAK Meter Reading property information -->
                <element name="FuelSystem_MeterReading"
                         minOccurs="0">
                    <complexType>
                        <all>
                            <!-- CreateFuel, when using pcat 2243751 to report meter reading/miles/hour incidents,
                             does not supply ADDREADING_EQUIPMENTCODE -->
                            <element name="ADDREADING_EQUIPMENTCODE"
                                     minOccurs="0"
                                     type="string"/>
                            <!-- UOM with trigger for meter-code -->
                            <element name="ADDREADING_UOM"
                                     type="string"/>
                            <!-- Current Meter reading -->
                            <element name="ADDREADING_CURRENT_METER_READING"
                                     type="string"/>
                            <!-- Current Meter reading Hours -->
                            <element name="ADDREADING_HOURS_METER_READING"
                                     type="string"/>
                            <!-- Vehicle-Meter-Reading Date-Time format mm/dd/yyyy hh:mm -->
                            <element name="ADDREADING_METER_READING_DATETIME"
                                     type="dateTime"/>
                        </all>
                    </complexType>
                </element>
                <!-- FuelSystem_TrakSync only needed for pcat:2243752 -->
                <element name="FuelSystem_TrakSync"
                         minOccurs="0">
                    <complexType>
                        <sequence>
                            <!-- Incident Request Detail for Apps.EAM.TrakSync pcat:2243752 ProcessTrakSync property information -->
                            <!-- person or vehicle -->
                            <element name="TRAK_TYPE" type="string"/>
                            <!-- + for active and - for inactive -->
                            <element name="TRAK_ACTION" type="string"/>
                            <!-- person code -->
                            <element name="PERSON_CODE" type="string"/>
                            <!-- person desc -->
                            <element name="PERSON_DESCRIPTION" type="string"/>
                            <!-- vehicle code -->
                            <element name="VEH_CODE" type="string"/>
                            <!-- vehicle desc -->
                            <element name="VEH_DESCRIPTION" type="string"/>
                            <!-- vehicle fillup miles -->
                            <element name="VEH_MAX_FILLUP_MILES" type="string"/>
                            <!-- vehicle tank capacity -->
                            <element name="VEH_TANK_CAPACITY" type="string"/>
                        </sequence>
                    </complexType>
                </element>
                <!-- Only used if the pcat incident area is unknown to this application -->
                <element name="Incident_Properties_Generic"
                         minOccurs="0">
